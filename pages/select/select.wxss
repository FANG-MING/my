.tou-list-container {
    align-items: flex-start;
}

.big-photo-list {
    position: absolute;
    margin-top: 150rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
}

.big-photo-item {
    width: 280rpx;
    height: 280rpx;
    text-align: center;
    padding: 20rpx;
}

.photo {
    width: 180rpx;
    height: 180rpx;
    background: #fff;
}

.tou-name {
    display: block;
    margin-top: 5px;
    font-size: 14px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-family: '华文新魏';
}

.bg {
    display: block;
    width: 100%;
    height: 620px;
}